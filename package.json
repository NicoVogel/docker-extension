{
  "name": "docker_extension",
  "version": "0.0.1",
  "description": "extend docker command line",
  "main": "doc.js",
  "preferGlobal": true,
  "bin": {
    "doc": "./dist/doc.js"
  },
  "scripts": {
    "build": "tsc -b tsconfig.json",
